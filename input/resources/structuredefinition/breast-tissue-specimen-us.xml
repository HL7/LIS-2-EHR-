<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="breast-tissue-specimen-us"/>
    <meta>
        <versionId value="13"/>
        <lastUpdated value="2021-03-26T18:15:11.776+00:00"/>
        <source value="#507Leq5Opqus4anq"/>
    </meta>
    <url value="http://www.hl7.org/fhir/us/cancer-reporting/StructureDefinition/breast-tissue-specimen-us"/>
    <name value="BreastTissueSpecimen"/>
    <title value="Breast Tissue Specimen"/>
    <status value="active"/>
    <description value="This is the breast tissue specimen that is being pathologically tested. Collection.bodySite contains the tumor site, otherwise referred to as the body location from where this specimen was obtained."/>
    <fhirVersion value="4.0.1"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Specimen"/>
    <baseDefinition value="http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-tumor-specimen"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Specimen">
            <path value="Specimen"/>
        </element>
        <element id="Specimen.collection">
            <path value="Specimen.collection"/>
            <min value="1"/>
            <max value="1"/>
            <mustSupport value="false"/>
        </element>
        <element id="Specimen.collection.bodySite">
            <path value="Specimen.collection.bodySite"/>
            <comment value="Note- Specimen.collection.bodySite may or may not be the same as the Tumor Site. Tumor Site (as it realtes to CAP eCC reporting to cancer registries SHALL be found in the Condition.bodySite, and may also be found in Specimen.collection.bodySite"/>
            <min value="1"/>
            <max value="1"/>
            <mustSupport value="false"/>
            <binding>
                <strength value="extensible"/>
                <valueSet value="http://www.hl7.org/fhir/us/cancer-reporting/ValueSet/breast-tumor-site-vs"/>
            </binding>
        </element>
        <element id="Specimen.collection.bodySite.extension">
            <path value="Specimen.collection.bodySite.extension"/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="Extension"/>
                <targetProfile value="http://www.hl7.org/fhir/us/cancer-reporting/StructureDefinition/laterality-lr"/>
            </type>
            <mustSupport value="true"/>
        </element>
    </differential>
</StructureDefinition>