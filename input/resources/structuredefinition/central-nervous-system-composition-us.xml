    </meta>
    <url value="http://www.hl7.org/fhir/us/cancer-reporting/StructureDefinition/central-nervous-system-composition-us"/>
    <name value="CentralNervousSystemComposition"/>
    <title value="Central Nervous System Composition"/>
    <status value="active"/>
    <description value="Central Nervous System Specimen Examination (CAP Version 4.0.0.0) Composition profile comprised of elements required for central registry reporting"/>
    <fhirVersion value="4.0.1"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Composition"/>
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Composition"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Composition">
            <path value="Composition"/>
        </element>
        <element id="Composition.type">
            <path value="Composition.type"/>
        </element>
        <element id="Composition.subject">
            <path value="Composition.subject"/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-patient"/>
            </type>
            <mustSupport value="true"/>
        </element>
        <element id="Composition.section">
            <path value="Composition.section"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="code"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
        </element>
        <element id="Composition.section:histologic_type">
            <path value="Composition.section"/>
            <sliceName value="histologic_type"/>
            <min value="0"/>
            <max value="*"/>
            <mustSupport value="false"/>
        </element>
        <element id="Composition.section:primary_cancer_condition">
            <path value="Composition.section"/>
            <sliceName value="primary_cancer_condition"/>
            <min value="0"/>
            <max value="1"/>
            <mustSupport value="false"/>
        </element>
        <element id="Composition.section:primary_cancer_condition.code">
            <path value="Composition.section.code"/>
            <min value="1"/>
            <max value="1"/>
            <patternCodeableConcept>
                <coding>
                    <system value="http://www.hl7.org/fhir/uv/cancer-reporting-common-library/CodeSystem/temp-codes"/>
                    <code value="XXX_primarycancercondition"/>
                    <display value="Primary Cancer Condition Section Code"/>
                </coding>
            </patternCodeableConcept>
            <mustSupport value="true"/>
        </element>
        <element id="Composition.section:molecular_markers">
            <path value="Composition.section"/>
            <sliceName value="molecular_markers"/>
            <mustSupport value="false"/>
        </element>
        <element id="Composition.section:molecular_markers.code">
            <path value="Composition.section.code"/>
            <min value="1"/>
            <max value="1"/>
            <patternCodeableConcept>
                <coding>
                    <system value="http://www.hl7.org/fhir/uv/cancer-reporting-common-library/CodeSystem/temp-codes"/>
                    <code value="XXX_biomarker"/>
                    <display value="Biomarker Section Code"/>
                </coding>
            </patternCodeableConcept>
            <mustSupport value="true"/>
        </element>
        <element id="Composition.section:molecular_markers.entry">
            <path value="Composition.section.entry"/>
            <min value="1"/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://www.hl7.org/fhir/us/cancer-reporting/StructureDefinition/cns-biomarker-observation-us"/>
            </type>
            <mustSupport value="true"/>
        </element>
        <element id="Composition.section:history_of_procedures">
            <path value="Composition.section"/>
            <sliceName value="history_of_procedures"/>
            <mustSupport value="false"/>
        </element>
        <element id="Composition.section:history_of_procedures.code">
            <path value="Composition.section.code"/>
            <min value="1"/>
            <max value="1"/>
            <patternCodeableConcept>
                <coding>
                    <system value="http://loinc.org"/>
                    <code value="47519-4"/>
                    <display value="History of Procedures Document"/>
                </coding>
            </patternCodeableConcept>
            <mustSupport value="true"/>
        </element>
        <element id="Composition.section:history_of_procedures.entry">
            <path value="Composition.section.entry"/>
            <min value="1"/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://www.hl7.org/fhir/us/cancer-reporting/StructureDefinition/cns-procedure-us"/>
            </type>
            <mustSupport value="true"/>
        </element>
    </differential>
