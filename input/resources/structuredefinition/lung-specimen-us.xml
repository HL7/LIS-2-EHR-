<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="lung-specimen-us"/>
    <meta>
        <versionId value="7"/>
        <lastUpdated value="2021-03-26T18:16:14.692+00:00"/>
        <source value="#QPBDDeqcJ9vM4tkm"/>
    </meta>
    <url value="http://www.hl7.org/fhir/us/cancer-reporting/StructureDefinition/lung-specimen-us"/>
    <name value="LungTumorSpecimen"/>
    <title value="Lung Tumor Specimen"/>
    <status value="active"/>
    <description value="This is the lung tissue specimen that is being pathologically tested. Collection.bodySite contains the tumor site, otherwise referred to as the body location from where this specimen was obtained."/>
    <fhirVersion value="4.0.1"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Specimen"/>
    <baseDefinition value="http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-tumor-specimen"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Specimen">
            <path value="Specimen"/>
        </element>
        <element id="Specimen.collection.bodySite">
            <path value="Specimen.collection.bodySite"/>
            <min value="1"/>
            <max value="1"/>
            <binding>
                <strength value="required"/>
                <valueSet value="http://www.hl7.org/fhir/us/cancer-reporting/ValueSet/lung-tumor-site-vs"/>
            </binding>
        </element>
        <element id="Specimen.collection.bodySite.extension">
            <path value="Specimen.collection.bodySite.extension"/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="Extension"/>
                <targetProfile value="http://www.hl7.org/fhir/us/cancer-reporting/StructureDefinition/laterality-lr"/>
            </type>
            <mustSupport value="true"/>
        </element>
    </differential>
</StructureDefinition>